#!/bin/bash

# dir
CACHE=cache
TMP=tmp

mkdir -p $CACHE
mkdir -p $TMP

# u-boot 
UBOOT_ENV=$CACHE/uboot.env

# kernel
KERNEL_VER=5.1.16
KERNEL_TAR=linux-${KERNEL_VER}.tar.xz
KERNEL_DEB=linux-image-${KERNEL_VER}_${KERNEL_VER}-1_arm64.deb

# node
NODE_TAG=v10.16.0
NODE_TAR=node-${NODE_TAG}-linux-arm64.tar.xz

# debase buld
DEBASE_TAR=debase.tar.xz
DEBASE_BUILD_TAR=debase-build.tar.xz
DEBASE_TESTING_TAR=debase-testing.tar.xz

# rootfs
ROOTFS_TAR=rootfs.tar
ROOTFS_TESTING_TAR=rootfs-testing.tar

# winas, winasd env
WINAS_ENV=$CACHE/winas.env
WINASD_ENV=$CACHE/winasd.env

